<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Supervision.Styles"
    xmlns:system="clr-namespace:System;assembly=mscorlib">


    <FontFamily x:Key="BaseFont">Franklin Gothic Book</FontFamily>
    <system:Double x:Key="Small">10</system:Double>
    <system:Double x:Key="Regular">12</system:Double>
    <system:Double x:Key="Big">16</system:Double>
    <system:Double x:Key="Hi">14</system:Double>

</ResourceDictionary>